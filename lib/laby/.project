;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: laby    description-file: {File :context ".package"}
    (<*>                                  tag-reference: {File :context "laby.project"})
    (<Project-Directory>                  directory: {Directory :context "src"}
      (<Project-Directory>                directory: {Directory :context "src" "laby"}
        (<Jazz-File>       name: client   source: {File :context "src" "laby" "client.jazz"})
        (<Jazz-File>       name: run      source: {File :context "src" "laby" "run.jazz"})
        (<Jazz-File>       name: window   source: {File :context "src" "laby" "window.jazz"})
        (<Jazz-File>       name: world    source: {File :context "src" "laby" "world.jazz"})
        (<Jazz-File>       name: zone     source: {File :context "src" "laby" "zone.jazz"})
        (<Jazz-File>       name: product  source: {File :context "src" "laby" "product.jazz"})
        (<Jazz-File>       name: windows  source: {File :context "src" "laby" "windows.jazz"})
        (<Jazz-File>       name: start    source: {File :context "src" "laby" "start.jazz"})
        (<Jazz-File>       name: script   source: {File :context "src" "laby" "script.jazz"})
        (<Jazz-File>       name: generate source: {File :context "src" "laby" "generate.jazz"}))))))
